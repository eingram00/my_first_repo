travel_log = [
{
  "country": "France",
  "visits": 12,
  "cities": ["Paris", "Lille", "Dijon"]
},
{
  "country": "Germany",
  "visits": 5,
  "cities": ["Berlin", "Hamburg", "Stuttgart"]
},
]
def add_new_country(country_visit, num_visits, cities_visit):
    new_dict = {}
    new_dict["country"] = country_visit
    new_dict["visits"] = num_visits
    new_dict["cities"] = cities_visit
    travel_log.append(new_dict)
add_new_country("Russia", 2, ["Moscow", "Saint Petersburg"])
print(travel_log)
